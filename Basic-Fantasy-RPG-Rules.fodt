       <text:p text:style-name="Table_20_Contents">Monster with Treasure</text:p>
      </table:table-cell>
     </table:table-row>
     <table:table-row>
      <table:table-cell table:style-name="Table261.A1" office:value-type="string">
       <text:p text:style-name="P522">85-88</text:p>
      </table:table-cell>
      <table:table-cell table:style-name="Table261.A1" office:value-type="string">
       <text:p text:style-name="Table_20_Contents">Special</text:p>
      </table:table-cell>
     </table:table-row>
