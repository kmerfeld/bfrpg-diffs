     <text:p text:style-name="P914"><text:a xlink:type="simple" xlink:href="#1%19Strongholds%19C%7Ctoxmark" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Strongholds<text:tab/>198</text:a></text:p>
