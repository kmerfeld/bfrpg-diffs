     <text:p text:style-name="P914"><text:a xlink:type="simple" xlink:href="#1%19Preparing%20Spells%20From%20Memory%19C%7Ctoxmark" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Preparing Spells From Memory<text:tab/>189</text:a></text:p>
     <text:p text:style-name="P914"><text:a xlink:type="simple" xlink:href="#1%19Magical%20Research%19C%7Ctoxmark" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Magical Research<text:tab/>190</text:a></text:p>
     <text:p text:style-name="P914"><text:a xlink:type="simple" xlink:href="#1%19Creating%20a%20Dungeon%20Adventure%19C%7Ctoxmark" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Creating a Dungeon Adventure<text:tab/>194</text:a></text:p>
     <text:p text:style-name="P914"><text:a xlink:type="simple" xlink:href="#2%19Traps%19C%7Ctoxmark" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Traps<text:tab/>196</text:a></text:p>
